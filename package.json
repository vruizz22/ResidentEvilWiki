{
  "dependencies": {
    "bulma": "^0.9.4",
    "sass": "^1.88.0"
  },
  "scripts": {
    "build:css": "sass ./app/assets/stylesheets/application.bulma.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "watch:css": "sass --watch ./app/assets/stylesheets/application.bulma.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules"
  }
}
